  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/jest": "^25.1.3",
    "@types/node": "^13.1.6",
    "jest": "^25.1.0",
    "ts-jest": "^25.2.1",
    "typescript": "^3.7.4"
  },
  "engines": {
    "node": ">=10.19.0"
  },
  "files": [
