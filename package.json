{
  "name": "@nonsugarless/manage-webp",
  "version": "1.0.0",
  "author": "Genki Sato",
  "bin": {
    "mwebp": "bin/index.js"
  },
  "bugs": {
    "url": "https://github.com/nonsugarless/manage-webp"
  },
  "dependencies": {
